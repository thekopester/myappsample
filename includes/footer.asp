<% select case (lcase(strPage))
 case "xallinventory","xnewinventory","xpreownedinventory","xclearanceinventory","xcustominventory","xfeaturedinventory" %>
<script type="text/javascript">
	$(document).ready(function () {

		$('.v7list-vehicle__button-list').each(function () {
			// Clone "Contact Us" button and have it be a call button :: FED KV102919
			var $contactUsLi = $(this).find('.v7list-vehicle__button-item--contact');
			var $callUsLi = $contactUsLi.clone();
			$callUsLi.addClass('invCallUs').removeClass('invContactUs');
			$callUsLi.find('a').removeAttr('data-modal-href').removeAttr('ismodal').attr('href', 'tel:605-305-4678');
			$callUsLi.find('.vehicle-button__text').html('Contact Us <strong>605-305-GORV</strong>');

			$callUsLi.insertBefore($contactUsLi);
			$contactUsLi.remove(); // remove default "Contact Us" button
		});

	});
</script>
<% end select %>

<% select case (lcase(strPage))
 case "xinventorydetail","xnewinventorydetail","xpreownedinventorydetail" %>
<script type="text/javascript">
	$(document).ready(function(){

		// Clone "Contact Us" button and have it be a call button :: FED KV102919
