<!-- BG Stretcher Script v2.0.1
====================== -->
<!-- <script type="text/javascript" src="/src/v4/js/bgstretcher.js"></script> -->
<script type="text/javascript" src="/js/bgstretcher.js"></script>
<script type="text/javascript">
		$(document).ready(function(){
			var images = Array();
			$('#bgStretchImages img').each(function() {
				images.push($(this).attr('src'));
			});
			// randomize slideshow image array
			var randImage = images[(Math.random() * images.length) | 0];
			$('div#bgWrapper').bgStretcher({
				images: [randImage],
				imageWidth: 1920,
				imageHeight: 556,
				slideShow: false,
				sequenceMode: 'normal',
				buttonPrev: '#prev',
				buttonNext: '#next',
				anchoring: 'center top',
				anchoringImg: 'center top'
			});
			var hrefs = new Array();
			$('#bgStretchImages a').each(function() {
				hrefs.push($(this).attr('href'));
			});
			$('#bgstretcher li').click(function() {
				//new tab for different sites, same tab for same site
				if (hrefs[$(this).index()].indexOf(window.location.protocol + "//" + window.location.hostname) == 0 || hrefs[$(this).index()].indexOf("/default.asp?page=") == 0) {
					window.location.assign(hrefs[$(this).index()]);
				} else if (hrefs[$(this).index()] != "" && hrefs[$(this).index()] != "#") {
					window.open(hrefs[$(this).index()]);
				}
			});
		});
</script>
<!-- END :: BG Stretcher Script v2.0.1  -->

<script type="text/javascript">
$( document ).ready(function() {

	// Initializing a Random Slide for Bootstrap Image Slideshow :: FED KV032719
	// source: https://www.sitepoint.com/full-screen-bootstrap-carousel-random-initial-image/

	// set slideshow item
	var slideItem = $('#Main-SlideShow .item');
	// set total number of slideshow image items
	var numberofSlides = $('#Main-SlideShow .item').length;
	// randomize slideshow image items
	var randomSlide = Math.floor((Math.random() * numberofSlides));

	// cycle through each image slideshow item and remove the .active class
	$('#Main-SlideShow .item').each(function(){
		$(this).removeClass('active');
	});
	// cycle through each image slideshow carousel indicator
	$('#Main-SlideShow .carousel-indicators li').each(function(){
		// remove the .active class from whichever carousel indicator item
		$(this).removeClass('active');
		// iterate through the carousel indicators and retrieve the value of the data-slide-to attribute
	  var slideValue = $(this).attr('data-slide-to');
		// If the random number matches the value of the data-slide-to attribute, we assign the active class to the corresponding slide and indicator
	  if(randomSlide == slideValue) {
	    $(this).addClass('active');
	    slideItem.eq(slideValue).addClass('active');
	  } else {
			// If that doesnâ€™t happen, we remove it from the target elements
	    $(this).removeClass('active');
	    slideItem.eq(slideValue).removeClass('active');
	  }
	});

	$('#Main-SlideShow').carousel({
		interval: 3000
		,pause: "hover"
	});

});
</script>

